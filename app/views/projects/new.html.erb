<h1>Create Project</h1>

<% if flash[:errors] %>
  <ul> 
    <% flash[:errors].each do |error| %>
      <li><%= error %></li>
    <% end %>
  </ul>
<% end %>

<%= form_with model: @project do |f| %>
Title: <%= f.text_field :title %><br>
Description: <%= f.text_area :summary %><br>
<%= f.submit %>
<% end %>
<%= button_to "Back", projects_path, method: :get %>